    "@doist/todoist-api-typescript": "2.1.2",
    "axios": "1.1.3",
    "crypto-js": "4.1.1",
    "dotenv": "16.0.3",
    "mongoose": "6.7.2",
    "twitter-api-v2": "1.12.9"
